# OMS gRPC API Environment Variables
# Copy this file to .env and update the values as needed

# ============================================
# Database Configuration
# ============================================
# Database host address
# For local development: localhost
# For Docker: postgres-service
DB_HOST=localhost

# Database port
# Default PostgreSQL port: 5432
# Local development default: 5433
DB_PORT=5433

# Database username
DB_USER=postgres

# Database password
DB_PASSWORD=postgres

# Database name
DB_NAME=oms

# ============================================
# gRPC Server Configuration
# ============================================
# gRPC server port
GRPC_PORT=8089

# gRPC host address (used by grpcui to connect to gRPC server)
# For local development: localhost
# For Docker: localhost (within container)
GRPC_HOST=localhost

# ============================================
# gRPC UI Configuration
# ============================================
# Enable/disable grpcui web interface
# Set to "true" to enable, "false" to disable
ENABLE_GRPCUI=true

# grpcui web interface port
GRPCUI_PORT=8080
